; 201
Name: mul 1
Pre: C < 0 && isPowerOf2(abs(C))
%Op0 = sub %Y, %X
%r = mul %Op0, C
  =>
%sub = sub %X, %Y
%r = mul %sub, abs(C)


; 203
Name: mul 2
Pre: C < 0 && isPowerOf2(abs(C))
%Op0 = add %Y, C1
%r = mul %Op0, C
  =>
%sub = sub -C1, %Y
%r = mul %sub, abs(C)


; Select:637
Name: select 1
%c = icmp eq %X, C
%r = select %c, %X, %Y
  =>
%c = icmp eq %X, C
%r = select %c, C, %Y


; Select:641
Name: select 2
%c = icmp ne %X, C
%r = select %c, %Y, %X
  =>
%c = icmp ne %X, C
%r = select %c, %Y, C


; InstCombineShift: 708
Name: shl 1
Pre: MaskedValueIsZero(%Op0, -1<<(width(C)-C))
%r = shl %Op0, C
  =>
%r = shl nuw %Op0, C


; InstCombineShift: 717
Name: shl 2
Pre: ComputeNumSignBits(%Op0) > C
%r = shl %Op0, C
  =>
%r = shl nsw %Op0, C


; InstCombineShift: 767
Name: lshr 1
Pre: MaskedValueIsZero(%Op0, (1<<C)-1)
%r = lshr %Op0, C
  =>
%r = lshr exact %Op0, C


; InstCombineShift: 812
Name: ashr 1
Pre: MaskedValueIsZero(%Op0, (1<<C)-1)
%r = ashr %Op0, C
  =>
%r = ashr exact %Op0, C
